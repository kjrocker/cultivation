{
	"name": "cultivation",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"package": "svelte-kit package",
		"preview": "vite preview",
		"prepare": "svelte-kit sync",
		"e2e": "playwright test",
		"test": "vitest",
		"test:ci": "yarn lint && vitest --run",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint .",
		"format": "prettier --write --plugin-search-dir=. .",
		"json": "ts-node --esm ./src/lib/data/dump-to-json.ts"
	},
	"devDependencies": {
		"@bulatdashiev/svelte-slider": "^1.0.3",
		"@playwright/test": "^1.28.1",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/adapter-static": "^2.0.1",
		"@sveltejs/kit": "^1.5.0",
		"@sveltejs/vite-plugin-svelte": "^2.0.2",
		"@types/jest": "^28.1.5",
		"@types/ramda": "^0.28.15",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"autoprefixer": "^10.4.7",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"jsdom": "^20.0.0",
		"postcss": "^8.4.14",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"svelte": "^3.54.0",
		"svelte-check": "^3.0.1",
		"svelte-preprocess": "^4.10.7",
		"tailwindcss": "^3.1.6",
		"ts-node": "^10.9.1",
		"tslib": "^2.4.1",
		"typescript": "^4.9.3",
		"vite": "^4.0.0",
		"vitest": "^0.25.3"
	},
	"type": "module",
	"dependencies": {
		"@steeze-ui/iconic-free": "^1.1.0",
		"@steeze-ui/radix-icons": "^1.1.0",
		"@steeze-ui/svelte-icon": "^1.3.1",
		"@tailwindcss/forms": "^0.5.2",
		"axios": "^0.27.2",
		"fast-xml-parser": "^4.0.9",
		"felte": "^1.2.2",
		"fsp-xml-parser": "^1.0.0",
		"ramda": "^0.28.0",
		"ramda-adjunct": "^3.2.0",
		"svelte-json-tree": "^1.0.0"
	}
}
